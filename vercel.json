{
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "regions": ["iad1"],
  "crons": [
    {
      "path": "/api/sync/dreps",
      "schedule": "0 */6 * * *"
    },
    {
      "path": "/api/sync/votes",
      "schedule": "15 */6 * * *"
    },
    {
      "path": "/api/sync/secondary",
      "schedule": "30 */6 * * *"
    },
    {
      "path": "/api/sync/slow",
      "schedule": "0 4 * * *"
    },
    {
      "path": "/api/admin/integrity/alert",
      "schedule": "0 */6 * * *"
    },
    {
      "path": "/api/admin/inbox-alert",
      "schedule": "0 3,9,15,21 * * *"
    },
    {
      "path": "/api/admin/api-health/alert",
      "schedule": "*/15 * * * *"
    }
  ]
}
